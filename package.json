{
  "name": "basic-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.20.0",
    "pnpm": ">=10.15.1"
  },
  "packageManager": "pnpm@10.15.1",
  "type": "module",
  "scripts": {
    "build": "pnpm db:deploy && next build",
    "check": "eslint . && tsc --noEmit",
    "db:dev": "prisma migrate dev --schema=./prisma/",
    "db:deploy": "prisma migrate deploy --schema=./prisma/",
    "db:push": "prisma db push --schema=./prisma/",
    "db:studio": "prisma studio --schema=./prisma/",
    "dev": "next dev --turbo -p3002",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate --schema=./prisma/",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "next:upgrade": "pnpm i next@latest react@latest react-dom@latest eslint-config-next@latest",
    "ui:upgrade": "for file in src/components/ui/*.tsx; do npx shadcn@latest add -y -o $(basename \"$file\" .tsx); done"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "better-auth": "^1.3.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.62.0",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.11",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.5.6",
    "eslint-plugin-import": "^2.32.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.17.1",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}
